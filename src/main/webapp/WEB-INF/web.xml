<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app >
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>abc</servlet-name>
  	<servlet-class>com.employee.AddData</servlet-class>
  </servlet>
    <servlet>
  	<servlet-name>abcd</servlet-name>
  	<servlet-class>com.employee.Excercise2</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>abc</servlet-name>
  	<url-pattern>/addData</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>abcd</servlet-name>
  	<url-pattern>/nextpage</url-pattern>
  </servlet-mapping>
</web-app>
